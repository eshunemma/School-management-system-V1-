generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int       @id @default(autoincrement())
  name       String
  email      String    @unique
  password   String
  userName   String    @unique
  role       String
  created_at DateTime? @default(now())
  created_by Int
  updated_at DateTime?
  update_by  Int?
}
